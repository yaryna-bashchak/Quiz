{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus": 0,
        "no-param-reassign": 0,
        "max-classes-per-file": [
            "error",
            { "ignoreExpressions": true, "max": 3 }
        ]
    }
}
